extends ../../layout-home

block content
  .container
    .row
      .col-md-12
        ol.breadcrumb
          li
            a(href="/home") Home
          li.active Token
        a(href="/api/tokens/addtoken")
          button.btn.btn-default.btn-lg
            | Add Token
    .row
      .col-md-12
        each accessToken in accessTokens
          p #{accessToken.serviceId.name} -> #{accessToken.token}